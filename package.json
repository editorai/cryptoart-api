{
  "name": "cryptoart-service",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "ENV=development PORT=3001 nodemon --harmony -e \".js,.json,.html\" app.js",
    "test": "ENV=test node ./models/sync_db.js &&  ENV=test nyc lab -m 30000 -v **/*.test.js -l",
    "coverage": "nyc report --reporter html && opener coverage/index.html",
    "upload-coverage": "nyc report --reporter json && codecov -f ./coverage/coverage-final.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "checketh": "node ./scripts/checketh.js",
    "deploy2oss": "scripts/deploy2oss.sh",
    "deployweb": "scripts/deployweb.sh",
    "deployservice": "scripts/deployservice.sh",
    "service": "ENV=development nodemon --harmony -e \".js,.json,.html\"  --ignore scripts/ app.js",
    "service-pro": "ENV=production nodemon --harmony -e \".js,.json,.html\" --ignore scripts/ app.js",
    "schedule": "ENV=development node ./scripts/schedule.js",
    "schedule-pro": "ENV=production node ./scripts/schedule.js",
    "download": "node ./scripts/downloadBlocks.js",
    "download2DB": "ENV=development node --harmony ./scripts/downloadBlocks2DB.js",
    "download2DB-pro": "ENV=production node --harmony ./scripts/downloadBlocks2DB.js"
  },
  "author": "macroshen",
  "license": "ISC",
  "dependencies": {
    "ali-oss": "^6.13.2",
    "axios": "^0.21.1",
    "base64url": "^3.0.1",
    "child_process": "^1.0.2",
    "codecov": "^3.8.1",
    "cors": "^2.8.5",
    "d3-queue": "^3.0.7",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-redis-cache": "^1.1.3",
    "moment": "^2.29.1",
    "node-schedule": "^2.0.0",
    "nyc": "^15.1.0",
    "opensea-js": "^1.1.11",
    "pg": "^8.5.1",
    "pg-format": "^1.0.4",
    "pg-hstore": "^2.3.3",
    "redis": "^3.0.2",
    "sequelize": "^6.5.0",
    "supertest": "^4.0.2",
    "tap": "^14.10.7",
    "uuid": "^8.3.2",
    "web3": "^1.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "nodemon": "^2.0.4",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^3.6.3"
  }
}
